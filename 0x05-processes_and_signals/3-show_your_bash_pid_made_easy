#!/bin/bash

# Get the PID of the current script
script_pid=$$

# Get the list of all processes
proc_list=$(</proc/self/stat)

# Extract the process name and PID for processes containing "bash"
while read -r pid comm; do
    if [[ "$comm" == *"bash"* ]]; then
        echo "$pid bash"
    fi
done <<< "$proc_list"
